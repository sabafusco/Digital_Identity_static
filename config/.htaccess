<IfModule mod_rewrite.c>

Options +FollowSymLinks
RewriteEngine on

	RewriteRule	^preference/user	http://external-rest-service/rest-preference-fe-rs-web/rest/api/preference/user [P]
	RewriteRule	^preference/category/(.*)	http://external-rest-service/rest-preference-fe-rs-web/rest/api/meta/metadatapreference/category/$1 [P]
	RewriteRule	^token	http://external-rest-service/rest-preference-fe-rs-web/rest/api/token [P]
	RewriteRule	^preference/user/salvapreferenze	http://external-rest-service/rest-preference-fe-rs-web/rest/api/preference/user/salvapreferenze [P]
        
        RewriteRule	^rest-intranet-services-rs-web/(.*)	http://external-rest-service/rest-intranet-services-rs-web/$1 [P]
        RewriteRule	^rest-user-informations-fe-rs/(.*) http://external-rest-service/rest-user-informations-fe-rs/$1 [P]

	RewriteRule	^section/header http://header:8080/html [P]
	RewriteRule	^section/footer http://footer:8080/html [P]
	RewriteRule	^section/atti http://atti:8080/html [P]
	RewriteRule	^section/newsavvisi http://newsavvisi:8080/html [P]		
	RewriteRule	^section/formazione http://formazione:8080/html [P]
	RewriteRule	^section/menuintranet http://menuintranet:8080/html [P]

       
</IfModule>
